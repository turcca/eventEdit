<h1>Listing secondary_prereqs</h1>

<table>
  <thead>
    <tr>
      <th>Event</th>
      <th>Prereq</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @secondary_prereqs.each do |secondary_prereq| %>
      <tr>
        <td><%= secondary_prereq.event_id %></td>
        <td><%= secondary_prereq.prereq_id %></td>
        <td><%= link_to 'Show', secondary_prereq %></td>
        <td><%= link_to 'Edit', edit_secondary_prereq_path(secondary_prereq) %></td>
        <td><%= link_to 'Destroy', secondary_prereq, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

